{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]  
  },
  "rules":{
    "noUnusedLocals": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-debugger": false
  },
  "jsRules": { //Disable for javascript
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-debugger": false
  } 
}
